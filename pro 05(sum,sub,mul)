#include<stdio.h>
int main()
{
    int sum,sub,mul,div,rem,a=60,b=30;
     sum=a+b;
     printf(" sum= %d\n",sum);
     sub=a-b;
     printf("sub= %d\n",sub);
     mul=a*b;
     printf("mul= %d\n",mul);
     div=a/b;
     printf("div= %d\n",div);
     rem=a%b;
     printf("rem= %d",rem);
    return 0;
}
